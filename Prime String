#include <bits/stdc++.h>


 

using namespace std;

bool isprime(int n)

{

    if(n<2)

    return false;

    if(n<4)

    return true;

    if(n%2==0||n%3==0)

    return false;

    for(int i=5;i*i<=n;i+=6)

    {

        if(n%i==0||n%(i+2)==0)

        return false;

    }

    return true;

}


 

int main() {

    int t;

    cin>>t;

    while(t--)

    {

        string s;

        cin>>s;

        map<char,int>mp;

        set<char>st;

        for(int i=0;i<s.length();i++)

        {

            mp[s[i]]++;

            st.insert(s[i]);

        }

        bool flag=true;

        if(isprime(st.size()))

        {

            for(auto it=mp.begin();it!=mp.end();it++)

            {

                if(!isprime(it->second))

                {

                    flag=false;

                }

            }

            if(flag==true)

            cout<<"YES"<<"\n";

            else

            cout<<"NO"<<"\n";

        }

        else

        cout<<"NO"<<"\n";

        

    }

    return 0;

}

